body
	min-width 1210px
	.fix
		width 50px
		height 50px
		position fixed
		right 30px
		bottom 50px
		display none
		z-index 99999
		font-size 20px
		text-align center
		border 1px solid #e6e6e6
		cursor pointer
		background #fff
		&:hover
			border-color #ff4883
			span
				color #ff4883
			i
				border-bottom-color #ff4883
		span 
			line-height 65px
			color #9e9e9e
		i
			position absolute 
			width 0
			height 0
			left 16px
			top 7px
			border-bottom 14px solid #9e9e9e
			border-right 9px solid transparent
			border-left 9px solid transparent
			
	.global
