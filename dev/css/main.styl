						z-index 2
						height 415px
						padding-left 20px
						width 430px
						left 225px
						border-left 1px solid #eaeaea
						border-right 1px solid #eaeaea
						border-bottom 1px solid #eaeaea
						padding-left 20px
						font-size 12px
						background #fff
						top 0
						& .inCl
							padding-top 15px
							border-bottom 1px solid #dfdfdf
							overflow hidden
							& h3
								font-size 12px
								line-height 15px
								color #313037
								display block
								margin-bottom 5px 
							&>a
								font-size 12px
								display inline-block
								float left
								padding 0 10px
								margin-bottom 7px
								border-right 1px solid #DFDFDF
								color #949494
								line-height 15px
								white-space: nowrap
								max-width 131px
								&:hover
									text-decoration underline
									color #005aa0
								
								
		& .rightCh
			width 984px
			height 460px
			float left
			background red
